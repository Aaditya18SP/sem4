m1=matrix(c(1,2,3,0,2,1,1,0,2),nrow=3,ncol=3,byrow=TRUE)
m2=matrix(c(1,0,2,2,0,1,1,0,1),nrow=3,ncol=3,byrow=TRUE)
m3=matrix(,nrow=3,ncol=3)
sum=0L
product=0L
print(m1)
print(m2)
print("the multiplication is:\n")
for(i in 1:3){
for(j in 1:3){
sum=0L
product=1L
for(k in 1:3){

product=m1[i,j]*m2[j,k]
sum=sum+product
m3[i,k]=sum
}
}
}
print(m3)

